import java.util.ArrayList;

public class Solution {
	public static ArrayList<Integer> rotateArray(ArrayList<Integer> arr, int k) {
        ArrayList<Integer>  
        arr1= new ArrayList<Integer>(arr.size());
        for(int i=k; i<arr.size(); i++)
            arr1.add(arr.get(i));
        for(int i=0; i<k; i++)
            arr1.add(arr.get(i));
            return arr1;
    }
}
